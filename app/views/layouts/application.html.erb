    </ul>
</nav>
<nav class="header-nav-right">
    <ul class="header-list-icon">

    </ul>
</nav>
</header>
<% if flash[:notice]%>
<div class="flash">
  <%= flash[:notice]%>
</div>
<% end %>
    <%= yield %>
  </body>
</html>

