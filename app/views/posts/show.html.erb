<div class="main posts-show">
  <div class="container">
    <div class="posts-show-item">
      <p>
      <%= @posts.each do |post| %>  
        <td><%= post.content %></td> 
      <% end %>
      </p>
      <div class="post-time">
        <%= @post.created_at %>
      </div>
      <div class="post-menus">
        <%= link_to("編集", "/posts/#{@post.id}/edit") %>
      </div>
    </div>
  </div>
</div>